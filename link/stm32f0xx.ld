

/* Linker script for STM32F031 */

/* Specify the memory areas */
MEMORY
{
  RAM_CODE (rx)   : ORIGIN = 0x20000000, LENGTH = DEFINED(RAM_CODE_SIZE) ? RAM_CODE_SIZE : 0k
  RAM_DATA (xrw)  : ORIGIN = 0x20000000 + LENGTH(RAM_CODE), LENGTH = RAM_SIZE - LENGTH(RAM_CODE)
  FLASH (rx)      : ORIGIN = 0x08000000 + FLASH_OFFSET, LENGTH = FLASH_SIZE - FLASH_OFFSET
}

INCLUDE "link/ldscript_gen.ld"
